<?xml version="1.0" encoding="UTF-8"?>
<root>
    <log>
        <log_level>DEBUG</log_level>
        <log_file_name>rpc_server</log_file_name>
        <log_file_path>/home/xiao/rapidrpc/log/</log_file_path>
        <log_sync_interval>500</log_sync_interval>
        <log_max_file_size>10000000</log_max_file_size>
    </log>

    <server>
        <ip>0.0.0.0</ip>
        <port>12345</port>
        <io_threads>4</io_threads>
    </server>
</root>

<!-- 
    log_level: 日志级别, DEBUG, INFO, ERROR
    log_file_name: 日志文件名
    log_file_path: 日志文件路径
    log_sync_interval: 日志同步间隔 ms
    log_max_file_size: 单个日志文件最大大小 bytes
 -->